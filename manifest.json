{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,

    "name": "Salt Link",
    "description": "This extnesion directs to links from Firebase server",
    "version": "1.0.2",
    "content_security_policy":"script-src 'self' https://ajax.googleapis.com https://www.gstatic.com/ https://salt-link.firebaseio.com; object-src 'self'",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "background": {
        "scripts": ["main.js"]
    }
  }
}